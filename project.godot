LootLockerNodeHandler="*res://Code/Web/LootLockerNodeHandler.cs"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"

[dotnet]

project/assembly_name="Godot-example-project-cs"

